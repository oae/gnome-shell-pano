{
  "compilerOptions": {
    "lib": ["ES2021"],
    "target": "ES2021",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "paths": {
      "@pano/*": ["./*"],
      "@gnome-shell/misc/util": ["./types/misc/util/index.d.ts"],
      "@gnome-shell/misc/animationUtils": ["./types/misc/animationUtils/index.d.ts"],
      "@gnome-shell/extensions/extension": ["./types/extension/extension.d.ts"],
      "@gnome-shell/extensions/prefs": ["./types/extension/prefs.d.ts"],
      "@gnome-shell/ui/layout": ["./types/ui/layout/index.d.ts"],
      "@gnome-shell/ui/main": ["./types/ui/main/index.d.ts"],
      "@gnome-shell/ui/messageTray": ["./types/ui/messageTray/index.d.ts"],
      "@gnome-shell/ui/lightbox": ["./types/ui/lightbox/index.d.ts"],
      "@gnome-shell/ui/dialog": ["./types/ui/dialog/index.d.ts"],
      "@gnome-shell/ui/modalDialog": ["./types/ui/modalDialog/index.d.ts"],
      "@gnome-shell/ui/popupMenu": ["./types/ui/popupMenu/index.d.ts"],
      "@gnome-shell/ui/panelMenu": ["./types/ui/panelMenu/index.d.ts"]
    }
  },
  "exclude": ["./dist"]
}
